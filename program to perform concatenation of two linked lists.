#include <stdio.h>
#include <stdlib.h>

struct Node {
    int data;
    struct Node* next;
};

struct Node* createNode(int value) {
    struct Node* newNode = (struct Node*)malloc(sizeof(struct Node));
    newNode->data = value;
    newNode->next = NULL;
    return newNode;
}

struct Node* insertEnd(struct Node* head, int value) {
    struct Node* newNode = createNode(value);

    if (head == NULL) {
        return newNode;
    }

    struct Node* temp = head;
    while (temp->next != NULL) {
        temp = temp->next;
    }

    temp->next = newNode;
    return head;
}

struct Node* concatenate(struct Node* head1, struct Node* head2) {

    if (head1 == NULL)
        return head2;

    if (head2 == NULL)
        return head1;

    struct Node* temp = head1;

    while (temp->next != NULL) {
        temp = temp->next;
    }

    temp->next = head2;

    return head1;
}

void display(struct Node* head) {
    struct Node* temp = head;
    while (temp != NULL) {
        printf("%d -> ", temp->data);
        temp = temp->next;
    }
    printf("NULL\n");
}

int main() {
    struct Node* list1 = NULL;
    struct Node* list2 = NULL;
    int n1, n2, value, i;

    printf("Enter number of nodes in first list: ");
    scanf("%d", &n1);

    for (i = 0; i < n1; i++) {
        printf("Enter value: ");
        scanf("%d", &value);
        list1 = insertEnd(list1, value);
    }

    printf("Enter number of nodes in second list: ");
    scanf("%d", &n2);

    for (i = 0; i < n2; i++) {
        printf("Enter value: ");
        scanf("%d", &value);
        list2 = insertEnd(list2, value);
    }

    printf("\nFirst Linked List: ");
    display(list1);

    printf("Second Linked List: ");
    display(list2);

  
    list1 = concatenate(list1, list2);

    printf("\nAfter Concatenation: ");
    display(list1);

    return 0;
}
